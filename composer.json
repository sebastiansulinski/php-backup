{
  "name": "sebastiansulinski/php-backup",
  "description": "Simple package for backing up databases, files and directories to Dropbox and Ftp.",
  "license": "MIT",
  "authors": [
    {
      "name": "Sebastian Sulinski",
      "email": "info@ssdtutorials.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.0",
    "league/flysystem": "^1.0",
    "spatie/flysystem-dropbox": "^1.0",
    "nesbot/carbon": "^2.0",
    "backup-manager/backup-manager": "^3.0",
    "illuminate/filesystem": "^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "ext-zip": "*"
  },
  "autoload": {
    "psr-4": {
      "SSD\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SSDTest\\": "tests/"
    }
  }
}
