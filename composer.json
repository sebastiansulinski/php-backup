{
  "name": "sebastiansulinski/php-backup",
  "description": "Simple package for backing up databases, files and directories to Dropbox and Ftp.",
  "license": "MIT",
  "authors": [
    {
      "name": "Sebastian Sulinski",
      "email": "info@ssdtutorials.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.2",
    "league/flysystem": "^3.0",
    "spatie/flysystem-dropbox": "^3.0",
    "nesbot/carbon": "^3.8",
    "backup-manager/backup-manager": "^3.1",
    "illuminate/filesystem": "^11.9",
    "league/flysystem-ftp": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0.1",
    "ext-zip": "*",
    "laravel/pint": "^1.18"
  },
  "autoload": {
    "psr-4": {
      "SSD\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SSDTest\\": "tests/"
    }
  }
}
